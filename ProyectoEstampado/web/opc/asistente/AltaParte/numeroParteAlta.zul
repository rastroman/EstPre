    <window title="Asistente para la creación de un número de parte "  id="WParteAsis" apply="controlador.Controller"  border="normal" style="background-color: #000000;">
        <separator height="10px" />
        <div align="center">
            <grid width="520px">
                <columns>
                    <column label="Instrucciones" width="250px"/>
                    <column label="Llenado" width="270px"/>
                </columns>
                <rows >
                    <row>	
                        <label value="Escribe el nombre de la pieza."/>
                        <div align="center">
                            <textbox id="nombreParEntrada" width="250px" value=""/> 
                        </div>
                    </row>
                    <row >	
                        <label value="Escribe el número de la pieza." />
                        <div align="center">
                            <textbox id="numeroParEntrada" width="250px" value=""/> 
                        </div>
                    </row>
                    <row >	
                        <label value="Escribe el código del AMEF de la pieza."/>
                        <div align="center">
                            <textbox id="amefEntrada" width="250px" value=""/>
                        </div>
                    </row>
                    <row>	
                        <label value="Selecciona el nivel de Ingeniería"/>
                        <div align="center">
                            <button id="reuNivI"   width="110px" height="39px" style=" font-weight:normal ;" label="Elegir" sclass="myWhite" mold="trendy" /> 
                            <listbox mold="select" width="250px" hflex="1" id="nivIngDisp" visible="false"/>
                        </div>
                    </row>
                    <row>	
                        <label value="Selecciona el modelo de la parte"/>
                        <div align="center">
                            <button id="modeloPar"  width="110px" height="39px" style=" font-weight:normal ;"  label="Elegir" sclass="myWhite" mold="trendy" /> 
                            <listbox mold="select"  width="250px" hflex="1" id="ModeDisp" visible="false"/>
                        </div>
                    </row>         
                </rows>
            </grid>
        </div>
        <separator height="10px" />
        <grid>
            <columns>
                <column label=""/>
                <column label=""/>
            </columns>
            <rows>
                <row>	
                    <image id="backimg" src="/img/atras1.png" /> 
                    <div align="right">
                        <image id="sigimg" src="/img/siguiente1.png"/> 
                    </div>
                </row>
                <row>	
                    <label value="Atras" />
                    <div align="right">
                        <label value="Siguiente"/>
                    </div>
                </row>
            </rows>
        </grid>
    </window> 
    <window height="1300px" title="Procesos" apply="controlador.Controller" id="fasesAsis" visible="false" border="normal" style="background-color: #000000;" >
        <div align="center">
            <button id="recargarFas" label="Iniciar"   style=" font-weight:normal ;"  sclass="myBlack" mold="trendy" />
        </div>
        <grid>
            <columns>
                <column label="Procesos"/>
                <column label=" "/> 
            </columns>
            <rows >
                <row>	
                    <cell rowspan="8" align="center">
                        <listbox id="leftf" height="840px" width="280px" fixedLayout="true" onDrop="move(event.dragged)" droppable="true" oddRowSclass="non-odd">
                            <listhead>
                                <listheader label="Procesos Anexados"/>
                            </listhead>
                        </listbox>
                    </cell>
                </row>
                <row align="center">	
                                              
                    <listbox id="rightf" height="120px" width="290px" fixedLayout="true" onDrop="move(event.dragged)" droppable="true" oddRowSclass="non-odd">
                        <listhead>
                            <listheader label="Entradas y Almacén"/>
                        </listhead>
                    </listbox> 
                          
                          
                </row>
                <row align="center">	
                                              
                    <listbox id="regresoInsp" height="120px" width="290px" fixedLayout="true" onDrop="move(event.dragged)" droppable="true" oddRowSclass="non-odd">
                        <listhead>
                            <listheader  label="Inspecciones"/>
                        </listhead>			
                    </listbox>     
                </row >
                <row align="center">	
                    <listbox id="regresoTrans" height="120px" width="290px" fixedLayout="true" onDrop="move(event.dragged)" droppable="true" oddRowSclass="non-odd">
                        <listhead>
                            <listheader label="Transporte"/>
                        </listhead>
                    </listbox>
                </row>
                <row align="center">	
                    <listbox  id="regresoPrepara" height="120px" width="290px" fixedLayout="true" onDrop="move(event.dragged)" droppable="true" oddRowSclass="non-odd">
                        <listhead>
                            <listheader label="Preparación"/>
                        </listhead>
                    </listbox>                            
                </row>
                <row align="center">	
                    <listbox  height="120px" id="regresoBlanking" width="290px" fixedLayout="true" onDrop="move(event.dragged)" droppable="true" oddRowSclass="non-odd">
                        <listhead>
                            <listheader label="Blanking"/>
                        </listhead>
                    </listbox>                            
                </row>
                <row align="center">	
                    <listbox  id="regresoTandem" height="120px" width="290px" fixedLayout="true" onDrop="move(event.dragged)" droppable="true" oddRowSclass="non-odd">
                        <listhead>
                            <listheader label="Tandem"/>
                        </listhead>
                    </listbox>                            
                </row>
                <row align="center">	
                    <listbox id="regresoTransfer" height="120px" width="290px" fixedLayout="true" onDrop="move(event.dragged)" droppable="true" oddRowSclass="non-odd">
                        <listhead>
                            <listheader label="Transfer"/>
                        </listhead>
                    </listbox> 
                </row>
            </rows>
        </grid>        
                
                
        
        <zscript><![CDATA[
        void move(Component dragged) {
            if (self instanceof Listitem) {
                self.parent.insertBefore(dragged, self);
            } else {
                self.appendChild(dragged);
            }
            
          void doDrop(DropEvent event, Component droped) {
			Component dragged = event.getDragged();
			droped.getParent().insertBefore(dragged, droped);
		}  
        }
    ]]>
        </zscript>   
