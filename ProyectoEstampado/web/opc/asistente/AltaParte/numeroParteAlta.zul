                    <label value="Nivel de Ingeniería"/>
                    <label id="nivelInge"/>
                </row>
                <row >
                    <label value="Nombre de Parte"/>
                    <label id="nombreParteR"/>   
                </row>
                <row>
                    <label value="Número de Parte"/>
                    <label id="numeroParterR"/>
                </row>
                <row visible="false" id="nivRow3">
                    <label value="Proveedor"/>
                    <label id="proveedors"/>
                </row>
                <row>
                    <label value="Asigne un Cliente directo de la pieza"/>
                    <listbox mold="select"  id="selCliente" hflex="1"  >
                    </listbox>
                </row>
                <row>
                    
                        
                    <button id="calculoAprov" label="Explotación" style=" font-weight:normal ;"  sclass="myWhite" mold="trendy"/>
                    
                    <div align="center">
                   
                        <label id="calculoAproLab" visible="false"/>
                        <label id="calculoAproL" visible="false"/>
                    </div>
                </row>
                <row>
                                          
                    <button id="calculoAprovT" label="Aprov Total" style=" font-weight:normal ;"  sclass="myWhite" mold="trendy"/>
                    
                    <div align="center">
                   
                        <label id="calculoAproLabT" visible="false"/>
                        <label id="calculoAproLT" visible="false"/>
                    </div>
                </row>
                <row id="nivRow4">
                    <label value="Peso de Rollo Utilizado"/>
                    
                    
                    <div align="center">
                   
                        <label id="pesoRolloUt"/>
                        <label id="pesoRolloUtD" />
                    </div>
                </row>
                
                    
            </rows>
        </grid>        
                
                
        
