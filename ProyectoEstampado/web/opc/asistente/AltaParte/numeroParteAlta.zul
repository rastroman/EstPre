    <window height="1300px" title="Asistente para la asignación de material para una pieza" apply="controlador.Controller" id="materialAsis" visible="false"  border="normal" style="background-color: #000000;" >
    
        <div align="center">
            <button id="accionMaterial" label="Mostrar" style=" font-weight:normal ;"  sclass="myBlack" mold="trendy"/>
             
        </div>
                
        <grid id="compGridMat" visible="false">
            <columns>
                <column label="Componente"/>
                <column label="Opción"/>
               
            </columns>
            <rows>
                                  
                
                <row>	
                    <label value="Selecciona un Material *" width="400px" />
                    <listbox mold="select"  id="matOpc" hflex="1"  >
                    </listbox>
                </row>
                <row id="nivRow1" visible="false"> 
                    <label value="Ancho del Material"/>
                    <div>
                        <textbox id="anchoMat" />
