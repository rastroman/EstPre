                    </div>
                </row>
                <row id="nivRow2" visible="false">
                    <label value="Espesor del Material"/>
                    <div>
                        <textbox id="espesorMat"/>
                        <label value="mm"/>
                    </div>
                </row>    
                <row>
                    <label value="Menciona el paso de Alimentación (Avance) del rollo " width="400px" />
                    <div>
                        <textbox  id="pasoAlEnt" value="" width="200px"/>
                        <label value=" mm" />
                    </div>
                </row>
                <row>
                    <label value="Tolerancia de ancho del rollo" width="400px"/>
                    <div>
                        <textbox value=" " id="tolEnt" width="200px"/>
                        <label value=" %"/>
                    </div>
                </row>
                <row>
                    <label value="Peso de Blanking *"/>
                    <div>
                        <textbox value="" id="pesoBlEnt" width="200px"/>
                        <label value=" kg"/>
                    </div>
                </row>    
                <row>
                    <label value="Peso de Parte *"/>
                    <div>
                        <textbox id="pesoParteEnt" width="200px"/>
                        <label value=" kg"/>
                    </div>
                </row>
                
                
